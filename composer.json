{
  "name": "dfridrich/php-mime-type",
  "type": "library",
  "description": "Simple PHP class for guessing file mime type based on file extension with ability to use in Symfony project.",
  "keywords": [
    "php",
    "mimetype"
  ],
  "homepage": "https://github.com/dfridrich/PhpMimeType",
  "license": "MIT",
  "authors": [
    {
      "name": "Dennis Fridrich",
      "email": "fridrich.dennis@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6|^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0",
    "symfony/http-foundation": "^2.8 || ^3.0"
  },
  "suggest": {
    "symfony/http-foundation": "Allows you to get prepared Symfony response."
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "Defr\\PhpMimeType\\": "src/",
      "Defr\\": "legacy/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "bin-dir": "bin"
  }
}
